// Boat dostupný zde: https://github.com/Libertas007/BoatLang

LOAD MATH;

SAIL ON YACHT;

REQUEST PACKAGE NUM;
BROADCAST "Zadejte počet iterací:"
LISTEN TO NUM;
REPACK NUM TO BARREL

LOOP NUM TIMES AS I:
	REQUEST PACKAGE MESSAGE;
	
	IF (MATH.MODULO I 3) == 0:
		ADD "FIZZ" TO MESSAGE;
	END;

	IF (MATH.MODULO I 5) == 0:
		ADD "BUZZ" TO MESSAGE;
	END;

	IF MESSAGE == "":
		SET MESSAGE TO (I.DECIMAL);
	END;
	
	BROADCAST MESSAGE;
END;